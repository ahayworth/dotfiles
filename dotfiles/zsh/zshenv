typeset -U path
[[ -d ~/bin ]] && path=(~/bin $path)
[[ -d /usr/local/opt/rbenv/shims ]] && path=(/usr/local/opt/rbenv/shims $path)

{%@@ if mac @@%}
postgres_bin=~/Applications/Postgres.app/Contents/Versions/latest/bin
[[ -d $postgres_bin ]] && path=($postgres_bin $path)
{%@@ endif @@%}

export GOPATH=~/projects/go
whence rbenv  >/dev/null && eval "$(rbenv init -)"
whence nodenv >/dev/null && eval "$(nodenv init -)"

fpath=($fpath ~/.zsh/fpath)
